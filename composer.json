{
    "name": "davekok/controller",
    "description": "A general purpose controller",
    "type": "project",
    "license": "UNLICENSE",
    "authors": [
        {
            "name": "Dave Kok",
            "email": "email@davekok.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "davekok\\controller\\": "src/",
            "davekok\\controller\\tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/davekok/log"
        },
        {
            "type": "vcs",
            "url": "https://github.com/davekok/stream"
        },
        {
            "type": "vcs",
            "url": "https://github.com/davekok/lalr1"
        },
        {
            "type": "vcs",
            "url": "https://github.com/davekok/http"
        }
    ],
    "require": {
        "davekok/stream": "dev-main",
        "davekok/lalr1": "dev-main",
        "davekok/http": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpunit/php-code-coverage": "^9.2.x-dev"
    }
}
