        },
        {
            "type": "vcs",
            "url": "https://github.com/davekok/webpackage"
        }
    ],
    "require": {
        "davekok/system": "dev-main",
        "davekok/kernel": "dev-main",
        "davekok/parser": "dev-main",
        "davekok/http": "dev-main",
        "davekok/webpackage": "dev-main",
        "psr/log": "^3.0"
